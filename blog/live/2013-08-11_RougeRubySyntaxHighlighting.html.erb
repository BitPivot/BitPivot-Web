<pre><code class='Ruby'>
class Cat
  def initialize(name)
    @name = name
  end

  def jump(ft)
    puts "#{@name} jumped #{ft}'"
  end
end

class SuperCat < Cat
  def jump
    puts 'I am Super Cat!'
    super(10)
  end
end

class LazyCat < Cat
  def jump
    puts 'Mrrrr...'
    super(1)
  end
end

lazycat = LazyCat.new('Big Nessie')
supercat = SuperCat.new('Splandles')

lazycat.jump()
supercat.jump()
</code></pre>